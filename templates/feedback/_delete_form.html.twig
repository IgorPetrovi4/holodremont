<form method="post" action="{{ path('feedback_delete', {'id': feedback.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ feedback.id) }}">
    <button class="btn"> <span style="font-size: 30px; color: Teal;">
                           <i class="fas fa-trash-alt"></i>
                        </span></button>
</form>
